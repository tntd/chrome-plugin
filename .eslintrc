{
	"extends": "tdued",
	"rules": {
		"space-before-function-paren": [
			"error",
			{
				"anonymous": "never",
				"named": "never",
				"asyncArrow": "always"
			}
		],
		"getter-return": "off",
		"for-direction": "off"
	},
	"globals": {
		"chrome": true,
		"document": true,
		"localStorage": true
	}
}
